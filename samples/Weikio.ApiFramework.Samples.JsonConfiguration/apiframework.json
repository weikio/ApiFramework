{
  "ApiFramework": {
    "Endpoints": {
      "/hello": {
        "Api": "Weikio.ApiFramework.Plugins.HelloWorld",
        "Tags": ["one", "another"]
      },
      "/another": {
        "Api": "Weikio.ApiFramework.Plugins.HelloWorld",
        "Name": "Anothers",
        "Group":"internal"
      },
      "/more": {
        "Api": "Weikio.ApiFramework.Plugins.HelloWorld",
        "Group": "external",
        "Description": "All the customers",
        "Tags": ["one", "another"]
      },
      "/mycustomer": {
        "Api": "Weikio.ApiFramework.Samples.JsonConfiguration",
        "Name": "Customers",
        "Description": "All the customers",
        "Tags": ["one", "another"]
      }
    }
  }
}